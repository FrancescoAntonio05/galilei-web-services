  "components": {
    "schemas": {
      "ConnectPlanetToStar": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          }
        }
      },
      "CreateGalaxyData": {
        "type": "object",
        "required": [
          "name",
          "description"
        ],
        "properties": {
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "CreatePlanetData": {
        "type": "object",
        "required": [
          "name",
          "capacity"
        ],
        "properties": {
          "capacity": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          },
          "name": {
            "type": "string"
          },
          "star_id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          }
        }
      },
      "CreateStarData": {
        "type": "object",
        "required": [
          "name",
          "nebula"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "nebula": {
            "type": "string"
          }
        }
      },
      "Galaxy": {
        "type": "object",
        "required": [
          "id",
          "name",
          "description",
          "user_id"
        ],
        "properties": {
          "description": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "user_id": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "Planet": {
        "type": "object",
        "required": [
          "id",
          "name",
          "capacity",
          "galaxy_id"
        ],
        "properties": {
          "capacity": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          },
          "galaxy_id": {
            "type": "string",
            "format": "uuid"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "star_id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          }
        }
      },
      "Star": {
        "type": "object",
        "required": [
          "id",
          "name",
          "nebula",
          "galaxy_id"
        ],
        "properties": {
          "galaxy_id": {
            "type": "string",
            "format": "uuid"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "nebula": {
            "type": "string"
          }
        }
      },
      "UpdateGalaxyData": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "UpdatePlanetData": {
        "type": "object",
        "properties": {
          "capacity": {
            "type": "integer",
            "format": "int64",
            "nullable": true,
            "minimum": 0
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "star": {
            "allOf": [
              {
                "$ref": "#/components/schemas/ConnectPlanetToStar"
              }
            ],
            "nullable": true
          }
        }
      },
      "UpdateStarData": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "nebula": {
            "type": "string",
            "nullable": true
          }
        }
      }
    }
